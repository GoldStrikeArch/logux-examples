{
  "extends": [
    "@logux/stylelint-config"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "import-normalize"
        ]
      }
    ],
    "selector-no-vendor-prefix": [
      true,
      {
        "ignoreSelectors": [
          "::-webkit-input-placeholder",
          "/-moz-.*/"
        ]
      }
    ]
  }
}
